{  
  "rewrites": [
        {
      "source": "/api/:path*",
      "destination": "https://e-commerce-store-backend-eta.vercel.app/:path*"
    },
        {
      "source": "/uploads/:path*",
      "destination": "https://e-commerce-store-backend-eta.vercel.app/:path*"
    },
       {
      "source": "/(.*)",
      "destination": "/index.html"
    }
 ]
}
